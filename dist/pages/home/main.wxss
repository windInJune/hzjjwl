.container .listBox .list .guardian .test {
  position: absolute;
  top: -10rpx;
  left: 20rpx;
  height: 24rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #333333;
}
.container .listBox .list .pickBtn {
  position: absolute;
  top: 50rpx;
  right: 20rpx;
  height: 80rpx;
  width: 80rpx;
}
